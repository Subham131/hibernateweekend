<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.hibernate.example.domain.Employee"
		table="emp_tbl">
		<id name="empId" type="int" column="emp_id_pk">
			<generator class="sequence">
				<param name="sequence_name">emp_sequence</param>
		</generator>
		</id>
		<property name="employeeName" column="emp_name" />
		<property name="age" column="emp_age" />
	</class>

</hibernate-mapping>